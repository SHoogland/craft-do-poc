{
    "name": "Craft Codespace",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "workspace",
    "runServices": [
      "composer",
      "backend",
      "mariadb",
      "webserver",
      "redis"
    ],
    "remoteUser": "codespace",
    "extensions": [
      "GitHub.vscode-pull-request-github",
      "ms-azuretools.vscode-docker"
    ],
    "workspaceFolder": "/"
  }